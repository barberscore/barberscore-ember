<div>
  {{outlet}}
</div>
<div class='table-responsive'>
  <table class='table'>
    <thead>
      <tr>
        <th>Draw</th>
        <th>Scheduled</th>
        <th>Group</th>
        <th>Song</th>
        <th>Prelim</th>
      </tr>
    </thead>
    <tbody>
      {{#each model.performances as |performance|}}
      <tr>
        <td>{{performance.draw}}</td>
        <td>{{moment-format performance.start_dt  'h:mm A'}}</td>
        <td>{{#link-to 'convention.session.round.group' performance.performer.group}}{{performance.performer.group.name}}{{/link-to}}</td>
        <td>
          {{#each performance.songs as |song|}}
            {{#if sess.isAuthenticated }}FOO{{/if}}{{song.tune.name}}<br>
          {{/each}}
        </td>
        <td>{{single-fixed performance.performer.prelim}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>