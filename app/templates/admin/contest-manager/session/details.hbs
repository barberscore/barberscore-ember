{{#bs-button-group as |bg|}}
  {{#bg.button onClick=(action 'openSession') class='form-group'}}Open {{model.kind}} Registration{{/bg.button}}
  {{#bg.button onClick=(action 'closeSession') class='form-group'}}Close {{model.kind}} Registration{{/bg.button}}
  {{#bg.button onClick=(action 'validateCurrent') class='form-group'}}Validate {{model.current.kind}}{{/bg.button}}
{{/bs-button-group}}
          {{#power-select
                options=model.contests
                selected=model.primary
                placeholder="Select Primary Contest"
                onchange=(action (mut model.primary))
                allowClear=true
                as |primary|
          }}
            {{primary.nomen}}
          {{/power-select}}
