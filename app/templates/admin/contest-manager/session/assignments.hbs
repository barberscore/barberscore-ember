<section>
  <div class='row'>
    <div class='col-md-3'>
      {{#bs-nav type="pills" justified=false stacked=true as |nav|}}
        {{#each sortedAssignments as |assignment|}}
          {{#nav.item}}
            {{#nav.link-to 'admin.contest-manager.session.assignments.assignment' assignment}}{{assignment.slot}} - {{assignment.person.common_name}}{{/nav.link-to}}
          {{/nav.item}}
        {{/each}}
      {{/bs-nav}}
    </div>
    <div class='col-md-9'>
      {{outlet}}
    </div>
  </div>
</section>
