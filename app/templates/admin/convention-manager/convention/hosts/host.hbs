<section>
  <div class='panel panel-default'>
    <div class='panel-heading'>
      {{!-- <ul class='list-inline pull-right'> --}}
        {{!-- <li>{{#bs-button onClick=(action 'previousItem' model)}}Previous{{/bs-button}}</li> --}}
        {{!-- <li>{{#bs-button onClick=(action 'nextItem' model)}}Next{{/bs-button}}</li> --}}
      {{!-- </ul> --}}
      <h4>
        {{model.entity.name}}
      </h4>
    </div>
    <div class='panel-body'>
      <div class='table-responsive'>
        <table class='table'>
          <caption>Awards</caption>
          <thead>
            <tr>
              <th>Name</th>
            </tr>
          </thead>
          <tbody>
            {{#each sortedAwards as |award|}}
              <tr>
                <td>
                  {{award.name}}
                </td>
              </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</section>
