<div class='row'>
<div class='col-md-6'>
{{#if model.picture}}
    <img src='{{model.picture}}' class='img-responsive img-thumbnail'>
{{else}}
    <img src='http://s3-us-west-1.amazonaws.com/barberscore/no_image_available.jpg' class='img-responsive img-thumbnail'>
{{/if}}
</div>
<div class='col-md-6'>
<h4>
  {{model.name}}
</h4>
<p>
  {{model.location}}
</p>
<p>
  {{model.facebook}}
</p>
<p>
  {{model.twitter}}
</p>
<p>
  {{model.email}}
</p>
</div>
</div>
