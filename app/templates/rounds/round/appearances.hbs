<section>
  {{!-- <div class='row'> --}}
    {{outlet}}
    {{!-- <div class='col-md-8'> --}}
      <table class='table'>
        <thead>
          <tr>
            <th class='col-md-1'>OA</th>
            <th class='col-md-1'>Scheduled</th>
            <th class='col-md-3'>Name</th>
            <th class='col-md-4'>Songs</th>
            {{!-- <th class='col-md-1'>Status</th> --}}
          </tr>
        </thead>
        <tbody>
          {{#each sortedAppearances as |appearance|}}
            <tr>
              <td>{{appearance.num}}</td>
              <td>{{moment-format appearance.slot.onstage 'LTS'}}</td>
              <td>{{#link-to 'rounds.round.appearances.appearance' appearance}}{{appearance.entry.entity.name}}{{/link-to}}</td>
              <td>
                {{sorted-songs songs=appearance.songs}}
              </td>
            </tr>
          {{else}}
            <tr>
              <td>(No entries.)</td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    {{!-- </div>
    <div class='col-md-4'>
    </div> --}}
  {{!-- </div> --}}
</section>