{{#bs-form formLayout="inline" action="saveRecord"}}
  {{#bs-form-group}}
    {{#power-select
      search=(action "searchChart")
      placeholder='Search by Chart Name.  Will also include arranger and ID Number if available.'
      selected=chart
      onchange=(action (mut chart))
      as |chart|
    }}
      {{chart.name}}
    {{/power-select}}
  {{/bs-form-group}}
  {{bs-button defaultText="Add" type="primary" buttonType="submit"}}
{{/bs-form}}
