<section>
    {{#if (eq model.status 'Verified')}}
      <ul class='list-styled'>
        <li>Announcements: {{fa-icon 'file-pdf-o'  size='2' click=(action 'clickAnnouncements') }}</li>
        <li>Official Scoring Summary: {{fa-icon 'file-pdf-o'  size='2' click=(action 'clickOss') }}</li>
        <li>Scoring Analysis: {{fa-icon 'file-pdf-o'  size='2' click=(action 'clickSa') }}</li>
        <li>Competitor Scoring Analyses: {{fa-icon 'file-pdf-o'  size='2' click=(action 'clickCsa') }}</li>
        {{#if (not-eq model.kind 'Finals')}}
          <li>Songs Sung Report: {{fa-icon 'file-pdf-o'  size='2' click=(action 'clickSung') }}</li>
        {{/if}}
      </ul>
    {{else if (eq model.status 'Finished')}}
      <ul class='list-styled'>
        <li>Official Scoring Summary: <a href={{model.oss}} target="_blank">{{fa-icon 'file-pdf-o'  size='2'}}</a></li>
        <li>Scoring Analysis: <a href={{model.sa}} target="_blank">{{fa-icon 'file-pdf-o'  size='2'}}</a></li>
      </ul>
    {{else}}
      <h4>
        Reports are available after the Round is Verified.
      </h4>
    {{/if}}
</section>
