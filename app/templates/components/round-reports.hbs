<section>
    {{#if (or (eq model.status 'Finished')(eq model.status 'Verified'))}}
      <ul class='list-styled'>
        <li>Announcements: &nbsp;<a href='{{announcements}}'>{{announcements}}&nbsp;{{fa-icon 'file-pdf-o'}}</a></li>
        <li>Official Scoring Summary: &nbsp;<a href='{{oss}}'>{{oss}}&nbsp;{{fa-icon 'file-pdf-o'}}</a></li>
