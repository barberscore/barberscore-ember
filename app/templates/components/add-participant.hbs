{{#bs-form formLayout="inline" action="saveParticipant"}}
  {{#bs-form-group}}
    {{#power-select
      options=organizationChoices
      placeholder='Choose Participating Organization'
      searchField='name'
      selected=organization
      onchange=(action (mut organization))
      as |organization|
    }}
      {{organization.name}}
    {{/power-select}}
  {{/bs-form-group}}
  {{bs-button defaultText="Add" type="primary" buttonType="submit"}}
{{/bs-form}}
