{{#bs-button
  type='primary'
  onClick=(perform sa)
}}
  {{#if sa.isRunning}}
    Downloading {{fa-icon "spinner" spin=true}}
  {{else if sa.last.isSuccessful}}
    Download Complete!
  {{else if sa.last.isError}}
    Error Downloading
  {{else}}
    Download SA&nbsp;&nbsp;{{fa-icon 'file-pdf-o'}}
  {{/if}}
{{/bs-button}}