<td>
  {{model.nomen}}
</td>
<td>
  {{#power-select
        search=(action "searchAssignment")
        placeholder="Enter Assignment Name"
        class='assignment-width'
        allowClear=true
        selected=model.judge
        onchange=(action (mut model.judge))
        as |judge|
  }}
      {{judge.nomen}}
  {{/power-select}}
</td>
<td>
  {{#power-select
    options=statusOptions
    placeholder='Status'
    allowClear=true
    selected=model.status
    onchange=(action (mut model.status))
    as |status|
  }}
    {{status}}
  {{/power-select}}
</td>