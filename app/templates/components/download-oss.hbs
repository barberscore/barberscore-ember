{{#bs-button
  type='primary'
  onClick=(perform oss)
}}
  {{#if oss.isRunning}}
    Downloading {{fa-icon "spinner" spin=true}}
  {{else if oss.last.isSuccessful}}
    Download Complete!
  {{else if oss.last.isError}}
    Error Downloading
  {{else}}
    Download OSS&nbsp;&nbsp;{{fa-icon 'file-pdf-o'}}
  {{/if}}
{{/bs-button}}