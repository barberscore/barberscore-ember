<section>
  <div class='panel panel-default'>
    <div class='panel-heading fixed-panel-header'>
      {{prev-next model=model sortedArray=sortedContests}}
      <h4>
        {{model.award.name}}
      </h4>
    </div>
    <div class='panel-body'>
      <section>
      {{#if model.notQualifier }}
        {{#power-select
          options=model.groupOptions
          selected=model.group
          onchange=(action (mut model.group))
          onclose=(action "saveModel" model)
          placeholder="Choose Award Winner"
          searchField='name'
          allowClear=true
          disabled=model.isDisabled
          as |group|
        }}
          Winner: {{group.name}}
        {{/power-select}}
      {{else}}
        (Qualifiers are determined by rule and appear on the OSS)
      {{/if }}
      </section>

      <section>

      </section>
    </div>
    <div class='panel-footer fixed-panel-footer'>
      {{!-- <ul class='list-inline'>
        {{#if (eq model.round.status 'Built')}}
          {{#if model.permissions.write}}
              <li>{{contest-delete model=model}}</li>
          {{/if}}
        {{/if}}
        <li class='pull-right'><h4>Contest Status: <strong>{{model.status}}</strong></h4></li>
      </ul> --}}
    </div>
  </div>
</section>
