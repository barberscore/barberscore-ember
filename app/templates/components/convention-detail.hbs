
{{#bs-form action="saveRecord"}}
  {{#bs-form-group}}
    {{#power-select selected=model.organization options=organizationChoices onchange=(action (mut model.organization)) as |name|}}
    {{name}}
    {{/power-select}}
  {{/bs-form-group}}
  {{#bs-form-group}}
    {{#power-select selected=model.status options=statusChoices onchange=(action (mut model.status)) as |name|}}
    {{name}}
    {{/power-select}}
  {{/bs-form-group}}
  {{#bs-form-group}}
    {{#power-select selected=model.kind options=kindChoices onchange=(action (mut model.kind)) as |name|}}
    {{name}}
    {{/power-select}}
  {{/bs-form-group}}
  {{bs-form-element controlType="text" value=model.organization}}
  {{!-- {{bs-form-element controlType="text" value=model.timezone}} --}}
  {{!-- {{bs-form-element controlType="text" value=model.org}} --}}
  {{bs-form-element controlType="text" placeholder="Location" value=model.location}}
  {{bs-button defaultText="Save" type="primary" buttonType="submit"}}
{{/bs-form}}
