{{#bs-button
  type='success'
  onClick=(perform contact)
}}
  {{#if contact.isRunning}}
    Downloading {{fa-icon "spinner" spin=true}}
  {{else if contact.last.isError}}
    Error Downloading
  {{else}}
    Download Group Contact Report&nbsp;&nbsp;{{fa-icon 'file-excel-o'}}
  {{/if}}
{{/bs-button}}