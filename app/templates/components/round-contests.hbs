{{#if (or (eq model.status 'Verified')(eq model.status 'Finished'))}}
  <div class='table-responsive'>
    <table class='table'>
        <caption>
          These are the contests that will be determined this round.
        </caption>
      <thead>
        <tr>
          <th class='col-md-1'>Num</th>
          <th class='col-md-5'>Name</th>
          <th class='col-md-1'>Manual</th>
          <th class='col-md-4'>Winner</th>
        </tr>
      </thead>
      <tbody>
        {{#each sortedContests as |contest|}}
          {{#if (lte contest.award.rounds model.num)}}
            <tr>
              <td>
                {{contest.num}}
              </td>
              <td>
                {{#link-to 'dashboard.conventions.convention.sessions.session.rounds.round.contests.contest' contest}}{{contest.award.name}}{{/link-to}}
              </td>
              <td>
                {{#if (eq contest.award.level 'Manual')}}
                  {{fa-icon 'check-circle' class='text-success'}}
                {{/if}}
              </td>
              <td>
                {{#if contest.group}}
                  {{contest.group.name}}
                {{else}}
                  (Not Yet Determined/Select Manually.)
                {{/if}}
              </td>
            </tr>
          {{/if}}
        {{else}}
          <tr>
            <td colspan=3>No contests.</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
{{else}}
  <h4>
    Winners are determined after the Round is Verified.
  </h4>
{{/if}}
