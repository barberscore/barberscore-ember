<section>
  {{#bs-form
    model=model
    formLayout='vertical'
    as |form|
  }}
    {{#form.element
      controlType='text'
      disabled=true
      placeholder='Kind'
      property='kind'
      as |el|
    }}
      Kind: {{el.value}}
    {{/form.element}}
    {{!-- {{#form.element
      controlType='text'
      disabled=true
      placeholder='Slots'
      property='slots'
      as |el|
    }}
      Slots: {{el.value}}
    {{/form.element}} --}}
  {{/bs-form}}

  {{#if model.ann_pdf}}
    <a href={{model.ann_pdf}}>Announcement Sheet</a>
  {{/if}}
</section>