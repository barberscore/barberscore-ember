<div class='col-md-10'>
  <section>
    <div class='panel panel-default'>
      <div class='panel-heading fixed-panel-header'>
        <h4>
          (New Entry)
        </h4>
      </div>
      <div class='panel-body'>
        <section>
          {{#bs-form formLayout="vertical" model=model onSubmit=(perform saveEntry) as |form| }}
            {{#form.element property='session' as |el|}}
              {{#power-select options=sessionOptions selected=el.value onchange=(action (mut el.value)) searchField='nomen' placeholder="Choose Session"
                allowClear=true noMatchesMessage="We Could Not Find an Open Session with that Name" as |session| }} Session: {{session.nomen}}; {{session.convention.venue.nomen}}; {{moment-format session.convention.startDate 'LL'}}
              {{/power-select}}
            {{/form.element}}
            {{#bs-button type='default' onClick=(action "clearEntry")}}Cancel{{/bs-button}}
            {{#bs-button type='success' buttonType='submit' }}Save{{/bs-button}}
          {{/bs-form}}
        </section>
        <section>
          <p>
            <strong>Note:</strong> You can enter any session currently listed as Open (including sessions from other Districts/Divisions other than your home).  If it shows "No Results Found" that means no sessions are currently opened.
          </p>
        </section>
      </div>
    </div>
  </section>
</div>
