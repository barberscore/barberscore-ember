{
  "name": "Barberscore Web",
  "description": "Scoring software for the Barbershop Harmony Society",
  "logo": "https://barberscore-api.herokuapp.com/static/api/bhs_logo.png",
  "repository": "https://github.com/barberscore/barberscore-web",
  "stack": "heroku-16",
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-ember-cli"
    },
    {
      "url": "https://github.com/hone/heroku-buildpack-static"
    }
  ],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "hobby"
    }
  },
  "env": {
    "ALGOLIASEARCH_API_KEY_SEARCH": {
      "description": "Algolia search key.",
      "required": true
    },
    "ALGOLIASEARCH_APPLICATION_ID": {
      "description": "Algolia app ID.",
      "required": true
    },
    "API_HOST": {
      "description": "The host of the Barberscore API.",
      "required": true
    },
    "API_NAMESPACE": {
      "description": "The API version.",
      "required": true
    },
    "AUTH0_CLIENT_ID": {
      "description": "The Auth0 Client id.",
      "required": true
    },
    "AUTH0_DOMAIN": {
      "description": "The Auth0 Client endpoint.",
      "required": true
    },
    "NPM_CONFIG_PRODUCTION": {
      "description": "Required by Heroku.  Do not edit.",
      "value": "false",
      "required": true
    },
    "HEROKU_APP_NAME": {
      "description": "Will be populated by Heroku in Review App.",
      "required": false
    },
    "HEROKU_PARENT_APP_NAME": {
      "description": "Will be populated by Heroku in Review App.",
      "required": false
    }
  },
  "environments": {
    "test": {
      "env": {
      },
      "scripts": {
        "test": "ember t"
      }
    }
  }
}
